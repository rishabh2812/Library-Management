    "                    ownlogin()\n",
    "                def librarian_submit():\n",
    "                        try:\n",
    "                            int(mobile.get())\n",
    "                            int(uid.get())\n",
    "                        except ValueError:\n",
    "                            MB.showinfo(\"Error\",\"UID and mobile number cant be characters Try again\")\n",
    "                            uid.delete(0,END)\n",
    "                            mobile.delete(0,END)\n",
    "                        if(int(mobile.get())<1000000000 or int(mobile.get())>9999999999):\n",
    "                            MB.showinfo(\"Error\",\"out of range value\")\n",
    "                            mobile.delete(0,END)\n",
    "                        elif(int(uid.get())>9999999999999999 or int(uid.get())<1000000000000000):\n",
    "                            MB.showinfo(\"Error\",\"Out of bound value\")\n",
    "                            uid.delete(0,END)\n",
    "                        else:\n",
    "                            cur=mydb.cursor()\n",
    "                            query=\"insert into librarian(lib_name,shift,uid,mobile,join_date)values(%s,%s,%s,%s,%s)\"\n",
    "                            cur.execute(query,(lib_name.get(),shift.get(),uid.get(),mobile.get(),join_date.get()))\n",
    "                            mydb.commit()\n",
    "                            MB.showinfo(\"Done\",\"Record Entered Successfuly\")\n",
    "                            lib_name.delete(0,END)\n",
    "                            mobile.delete(0,END)\n",
    "                            uid.delete(0,END)\n",
    "                            join_date.delete(0,END)\n",
    "                            \n",
    "                librarian=Tk()\n",
    "                librarian.geometry('600x700')\n",
    "                librarian.resizable(0,0)\n",
    "                librarian.title('librarian account')\n",

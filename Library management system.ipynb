    "mydb=mysql.connect(host=\"localhost\",user=\"root\",password=\"1234\",database=\"library\")\n",
    "cur=mydb.cursor()\n",
    "def ownlogin():\n",
    "    def ownerloginsuccessful():\n",
    "        cur.execute('select id,password from ownerlogin')\n",
    "        credential=cur.fetchone()\n",
    "        if(ownerid.get() in credential and ownerpwd.get() in credential):\n",
    "            ownerlogin.destroy()\n",
    "            MB.showinfo(\"LOGIN SUCCESSFUL\",\"SUCCESSFULLY LOGGED IN\")\n",
    "            def librarianwindow():\n",
    "                loginsuccessful.destroy()\n",
    "                def back():\n",
